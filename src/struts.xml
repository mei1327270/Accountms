<struts>

	<package name="hello" extends="struts-default">
		<!-- Spring整合Struts2的時候，在struts2中配置的Spring的Action的class需要IOC容器中該bean的id -->
		<action name="account-*" class="accountAction" method="{1}">
		</action>
	</package>

</struts>
